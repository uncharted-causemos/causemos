{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run --coverage.enabled=true",
    "test:ui": "vitest --ui --coverage.enabled=true",
    "lint": "eslint . --fix --ext .js,.ts,.vue",
    "lint:ci": "eslint . --ext .js,.ts,.vue",
    "postinstall": "check-node-version --package",
    "prep": "vue-tsc && npm run lint && npm run test:ci"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "!*.{js,ts,vue}": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@uncharted.software/facets-core": "^3.1.0",
    "@uncharted.software/facets-plugins": "^3.1.0",
    "@uncharted.software/lex": "^1.0.0",
    "axios": "^0.21.4",
    "check-node-version": "^4.0.1",
    "core-js": "^3.20.3",
    "cropro": "^1.1.0",
    "d3": "^6.3.0",
    "d3-interpolate-path": "^2.1.2",
    "docx": "^6.0.3",
    "element-resize-detector": "^1.1.14",
    "file-saver": "^2.0.5",
    "flatpickr": "^4.5.2",
    "floating-vue": "^2.0.0-beta.24",
    "font-awesome": "^4.7.0",
    "html2canvas": "^1.0.0-rc.7",
    "jquery": "^3.3.1",
    "lodash": "^4.17.21",
    "maplibre-gl": "^2.4.0",
    "markerjs2": "^2.13.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "mousetrap": "^1.6.2",
    "pdfjs-dist": "^2.10.377",
    "pptxgenjs": "^3.5.0",
    "preact": "^8.3.1",
    "qs": "^6.5.2",
    "shepherd.js": "^8.3.1",
    "svg-flowgraph": "0.5.5",
    "uuid": "^8.3.2",
    "v-tooltip": "^4.0.0-alpha.1",
    "vsup": "^1.1.0",
    "vue": "^3.2.29",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.0-0",
    "vue-toastification": "^2.0.0-rc.1",
    "vue3-resize": "^0.2.0",
    "vuex": "^4.0.0-0",
    "vuex-router-sync": "^6.0.0-rc.1"
  },
  "devDependencies": {
    "@types/d3": "^6.3.0",
