{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.x <=18.x"
  },
  "scripts": {
    "postinstall": "check-node-version --package",
    "start": "node ./src//bin/www",
    "debug": "node --inspect ./src//bin/www --log-level debug",
    "dev": "PORT=3001 nodemon --inspect ./src//bin/www --log-level debug",
    "lint": "eslint ./src --fix",
    "lint:ci": "eslint ./src",
    "test": "mocha 'test/unit/**/*.js'"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "!*.js": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.4.0",
    "aws-sdk": "^2.510.0",
    "body-parser": "^1.18.3",
    "check-node-version": "^4.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.0",
    "express-async-handler": "^1.1.4",
    "express-http-proxy": "^1.6.0",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.2",
    "js-combinatorics": "^0.5.3",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "lru-cache": "^5.1.1",
    "moment": "^2.22.2",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "nocache": "^2.0.0",
    "request": "^2.88.0",
    "sharp": "^0.30.3",
    "uuid": "^8.3.2",
    "winston": "^3.1.0",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^5.2.0",
    "nodemon": "^2.0.7"
  }
}
